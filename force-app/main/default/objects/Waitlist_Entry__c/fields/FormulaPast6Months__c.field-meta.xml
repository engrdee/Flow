<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FormulaPast6Months__c</fullName>
    <externalId>false</externalId>
    <formula>AND
(
    DATEVALUE(CreatedDate) &gt; ADDMONTHS(TODAY(),-6),
    DATEVALUE(CreatedDate) &lt; TODAY()
)</formula>
